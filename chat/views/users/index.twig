{% extends 'templates/default.twig' %}

{% block title %}Manage Users{% endblock %}

{% block content %}
  
    <div class="row ">
            <div class="col-md-12">
         
                  
                  <div class="row twenty-padding">
                      {% if users %}
                        {% include 'users/partials/users.twig' %} 
                      {% else %}
                        <div class="alert alert-info alert-dismissable text-center" style="padding-top: 5px;padding-bottom: 5px;">
                            <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
